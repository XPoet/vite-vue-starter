.nav-box {
  position relative
  box-sizing border-box
  width 100%
  height 100%

  .nav-item {
    position relative
    box-sizing border-box
    width 100%
    height 60px
    padding 10px
    display flex
    align-items center
    cursor pointer

    &.active {
      background #f2f2f2
    }
  }
}
